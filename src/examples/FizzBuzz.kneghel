class FizzBuzz {
    fun main(args) {
        i = 1
        while i<=100{
            if i%15==0{
                _= print("FizzBuzz")
            } else if i %3 ==0 {
                _ =print("Fizz")
            } else if i% 5== 0 {
                _=print(" - Buzz")
            } else {
                _ = print(i)
            }

            i=i+1
        }
        return 0
    }
}