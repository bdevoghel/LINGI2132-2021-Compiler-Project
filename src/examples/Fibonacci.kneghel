class Fibonacci {

    /*
     * Recursive fibonacci function
     */
    fun fibonacci(a, b, N) {
        if N == 0 {
            return null // end of recursion
        }
        _ = print(a)
        return fibonacci(b, a+b, N-1)
    }

    fun main(args) {
        N = int(args[0])
        _ = fibonacci(0, 1, N)
        return 0
    }
}