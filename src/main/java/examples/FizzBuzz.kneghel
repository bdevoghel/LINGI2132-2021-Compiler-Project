// this function is different from the Java one, because we want to testclass FizzBuzz {
    fun main(args) {
        i = 1
        while i<=100{
            if i%15==0{
                println("FizzBuzz")
            } else if i %3 ==0 {
                print("Fizz")
            } else if i% 5== 0 {
                print(" - Buzz")
            } else {
                print(i)
            }

            i=i+1
        }
    }
}