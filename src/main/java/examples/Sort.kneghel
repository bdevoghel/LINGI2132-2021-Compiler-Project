class Sort {

    fun swap(a, i, j) {
        tmp = a[i]
        a[i] = a[j]
        a[j] = tmp
    }

    fun sort(numbers) {
        i = 0
        while i < len(numbers) {
            j = i+1
            while j < len(numbers) {
                if numbers[i] > numbers[j] {
                    swap(numbers, i, j)
                }
                j = j + 1
            }
            i = i + 1
        }
    }

    fun main(args) {
        numbers[] = [] * len(args);
        i = 0
        while i < len(args) {
            numbers[i] = int(args[i])
            i = i + 1
        }
        sort(numbers)
        i = 0
        while i < numbers.length {
            print(numbers[i])
            i = i + 1
        }
    }
}
